									"var jsonData = pm.response.json();",
									"if (jsonData.token) {",
									"    pm.environment.set(\"token\", jsonData.token);",
									"    console.log(\"Token updated: \" + jsonData.token);",
									"}"
								],
								"type": "text/javascript"
							}
						}
